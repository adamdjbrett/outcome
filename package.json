{
	"name": "outcome",
	"version": "1.0.1",
	"description": "Outcome Doctrine of Discovery",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-search": "npx @11ty/eleventy && npx pagefind --site _site --force-language unknown",
		"build-prod": "cross-env ELEVENTY_ENV=production npx @11ty/eleventy && npx pagefind --site _site --force-language unknown",
		"dev:search": "npm run build-search && npx pagefind --site _site --serve",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/outcome/",
		"build-ghpages-prod": "cross-env ELEVENTY_ENV=production npx @11ty/eleventy --pathprefix=/outcome/ && npx pagefind --site _site --force-language unknown",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/outcome/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"prebuild-prod": "rm -rf _site && rm -rf .cache"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/adamdjbrett/outcome.git"
	},
	"author": {
		"name": "Adam DJ Brett",
		"email": "info@adamdjbrett.com",
		"url": "https://adamdjbrett.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"homepage": "https://outcome.doctrineofdiscovery.org/",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^5.0.0",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-rss": "^2.0.2",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"clean-css": "^5.3.3",
		"cross-env": "^7.0.3",
		"cssnano": "^7.1.1",
		"csso-cli": "^3.0.0",
		"luxon": "^3.5.0",
		"pagefind": "^1.4.0",
		"purgecss": "^6.0.0",
		"terser": "^5.31.1",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.3.1"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.1",
		"citation-js": "^0.7.16",
		"eleventy-plugin-pwa-v2": "^1.0.2",
		"eleventy-plugin-toc": "^1.1.5",
		"globby": "^16.1.0",
		"markdown-it": "^14.1.0",
		"markdown-it-anchor": "^9.2.0"
	}
}
